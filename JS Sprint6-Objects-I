const products = { 
  apple: { type: 'fruit', price: 1.32 }, 
  banana: { type: 'fruit', price: 0.25 },
  potato: { type: 'vegetable', price: 0.98 },
};
for (let key in products) {
if (products[key].type === 'fruit') {
  if (products[key].type === 'fruit') {
    products[key].price *= 1.10;
  }
 }
}
Object.keys(products) // apple, banana, potato
